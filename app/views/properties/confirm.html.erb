<h3>Send with the following contents.</h3>
<%= form_with(model: @property, url: properties_path, local: true) do |form| %>
  <%= form.hidden_field :name %>
  <%= form.hidden_field :rent %>
  <%= form.hidden_field :address %>
  <%= form.hidden_field :year %>
  <%= form.hidden_field :memo %>
  <p>name:<%= @property.name %></p>
  <p>rent:<%= @property.rent %></p>
  <p>address:<%= @property.address %></p>
  <p>year:<%= @property.year %></p>
  <p>remarks:<%= @property.memo %></p>
  <%= form.submit  "confirm" %>
  <%= form.submit "back", name: 'back' %>
<% end %>
